var ar = ["15.01.2018",
"12.02.2018",
"12.03.2018",
"09.04.2018",
"14.05.2018",
"04.06.2018",
"15.01.2018",
"21.05.2018",
"29.01.2018",
"26.02.2018",
"26.03.2018",
"16.04.2018",
"28.05.2018",
"25.06.2018",
"22.01.2018"];
ar.sort(
function(a,b){
  return new Date(a.split(".")[2], a.split(".")[1] - 1, a.split(".")[0])-new Date(b.split(".")[2], b.split(".")[1] - 1, b.split(".")[0]);
}
)
